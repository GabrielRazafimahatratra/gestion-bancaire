generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  numeroCompte       String   @id
  nomClient          String
  prenomsClient      String
  addresseClient     String
  emailClient        String
  telephoneClient    String
  montantClient      Float
}
